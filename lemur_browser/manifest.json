{
  "manifest_version": 2,
  "name": "Time Accelerator",
  "version": "1.0",
  "description": "Toggle 1000x time acceleration on any website",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
